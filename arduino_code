#include <SoftwareSerial.h>
#include "DFRobotDFPlayerMini.h"

SoftwareSerial mySoftwareSerial(10, 11); 
DFRobotDFPlayerMini myDFPlayer;

void setup() {

  Serial.begin(9600);
  mySoftwareSerial.begin(9600); 
  
  delay(500); 

  myDFPlayer.volume(30); 
  myDFPlayer.play(1); 
  myDFPlayer.loop(1); 
}

void loop() {

}
